    "                    \"Final\":final_price,})\n",
    "\n",
    "#Comput price change%\n",
    "ava_df['Percentage Price Change']= (ava_df['Final']-ava_df['Initial'])/(ava_df['Initial'])\n",
    "ava_df['Percentage Price Change']=(ava_df['Percentage Price Change']*100).map(\"{:.2f}%\".format)\n",
    "ava_df"
